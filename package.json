{
  "name": "@softwareventures/chain",
  "version": "0.0.0-development",
  "description": "A simple identity monad to enable function chaining",
  "keywords": [
    "TypeScript",
    "chain",
    "identity",
    "monad"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/chain",
  "bugs": "https://github.com/softwareventures/chain/issues",
  "repository": "github:softwareventures/chain",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "devDependencies": {
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "5.1.0",
    "@softwareventures/tslint-rules": "1.0.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.7",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typescript": "4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
